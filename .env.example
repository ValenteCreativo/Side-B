# Database
DATABASE_URL="file:./dev.db"

# Story Protocol
# Get API keys from https://www.story.foundation/build
# Using Aeneid testnet for development
STORY_PRIVATE_KEY="your_wallet_private_key_here"
STORY_RPC_URL="https://aeneid.storyrpc.io"

# Coinbase Embedded Wallets
# Get API keys from https://docs.cdp.coinbase.com/
COINBASE_API_KEY="your_coinbase_api_key_here"
COINBASE_API_SECRET="your_coinbase_api_secret_here"
COINBASE_PROJECT_ID="your_project_id_here"

# Platform Wallet (Side B Revenue)
# This wallet receives the 2% platform fee from each transaction
PLATFORM_WALLET_ADDRESS="your_platform_wallet_address_here"
NEXT_PUBLIC_PLATFORM_WALLET="your_platform_wallet_address_here"

# Base Network RPC (Payment Verification)
# Options: https://mainnet.base.org or https://base.drpc.org
BASE_RPC_URL="https://mainnet.base.org"

# Halliday Payments (Fiat On-ramp)
# Get API key from https://halliday.xyz
HALLIDAY_API_KEY="Your_api_key_here"

# Pinata IPFS
# Get JWT from https://pinata.cloud
PINATA_JWT="your_pinata_jwt_here"

# BaseScan API (Transaction History)
# Get API key from https://basescan.org/myapikey
BASESCAN_API_KEY="your_basescan_api_key_here"

# Next.js
NEXT_PUBLIC_APP_URL="http://localhost:3000"
